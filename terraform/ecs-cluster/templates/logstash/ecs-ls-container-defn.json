[
    {
        "name": "${NAME}",
        "image": "${IMAGE}",
        "essential": true,
        "portMappings": [
            {
                "containerPort": 5044
            },
            {
                "containerPort": 9600
            }
        ],
        "environment": [
            {
                "name": "OPENSEARCH_HOSTS",
                "value": "${OPENSEARCH_HOSTS}"
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${LOG_GROUP}",
                "awslogs-region": "${REGION}",
                "awslogs-stream-prefix": "${STREAM_PREFIX}"
            }
        }
    }
]